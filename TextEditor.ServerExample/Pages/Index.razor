@page "/"
@using TextEditor

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.

<SurveyPrompt Title="How is Blazor working for you?" />

@{
    var model = new TextEditorModel();
    model.SetText("test...\n...tset");
    model.TooltipList = new();
    model.TooltipList.Add(new TextEditorTooltip(
        startPositionIndex: 0,
        endPositionIndex: 1,
        foreignKey: 0,
        byteKind: TextEditorModel.CharacterTooltipByteKind));
    model.EnableDecorations();
    model.ReceiveKeyboardDebounce();
}

<TextEditorComponent Model="model"/>
